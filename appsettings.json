{
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=app.db"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "AppSettings": {
    "ExportFilePath": "C:\\Exports",
    "ComputationSettings": {
      "MaxEntries": 10000000,
      "InitialOddLimit": 2500000
    }
  }
}
